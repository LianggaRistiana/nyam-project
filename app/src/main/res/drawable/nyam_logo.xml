                    android:pathData="M 146.086 175.546 C 166.665 187.323 183.456 119.863 183.456 119.863 L 171.15 108.128 L 176.969 83.633 L 211.13 83.633 L 211.13 177.43 C 211.13 191.077 200.026 202.139 186.33 202.139 L 75.574 202.139 L 75.574 108.341 C 75.574 94.695 86.677 83.633 100.374 83.633 L 165.473 83.633 L 146.086 175.546 Z M 102.973 168.03 C 102.973 168.03 120.202 100.68 140.704 112.59 L 120.001 202 L 110.001 202 L 115.203 179.845 L 102.973 168.03 Z"
